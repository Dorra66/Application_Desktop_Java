          <TableColumn fx:id="nameEvent" prefWidth="203.0" text="Event" />
          <TableColumn fx:id="dateEvent" prefWidth="277.0" text="Date" />
        </columns>
      </TableView>
      <Button fx:id="suppparticip" layoutX="220.0" layoutY="370.0" mnemonicParsing="false" onAction="#supprimerMaParticipation" text="Delete Participation" />
      <Label layoutX="517.0" layoutY="167.0" text="FeedBack Description" textFill="BLUE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <TextArea fx:id="descripavis" layoutX="662.0" layoutY="67.0" prefHeight="200.0" prefWidth="200.0" promptText="Insert feed_back" />
