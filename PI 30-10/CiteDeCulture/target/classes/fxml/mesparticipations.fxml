<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="875.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.MesparticipationsController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="875.0" layoutY="42.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../../../../images/cite.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tableEvent" layoutX="26.0" layoutY="49.0" prefHeight="307.0" prefWidth="481.0">
